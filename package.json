{
  "name": "my-new-app",
  "productName": "my-new-app",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "lzyang1995",
    "email": "lzyang1995@outlook.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "my_new_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/main_window/index.html",
                  "js": "./src/main_window/renderer.tsx",
                  "name": "main_window"
                },
                {
                  "html": "./src/webview_menu_config/index.html",
                  "js": "./src/webview_menu_config/renderer.tsx",
                  "name": "webview_menu_config"
                },
                {
                  "html": "./src/protocol_config/index.html",
                  "js": "./src/protocol_config/renderer.tsx",
                  "name": "protocol_config"
                },
                {
                  "html": "./src/test_win/index.html",
                  "js": "./src/test_win/renderer.tsx",
                  "name": "test_win"
                },
                {
                  "html": "./src/terminal/index.html",
                  "js": "./src/terminal/renderer.tsx",
                  "name": "terminal"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.52",
    "@electron-forge/maker-deb": "6.0.0-beta.52",
    "@electron-forge/maker-rpm": "6.0.0-beta.52",
    "@electron-forge/maker-squirrel": "6.0.0-beta.52",
    "@electron-forge/maker-zip": "6.0.0-beta.52",
    "@electron-forge/plugin-webpack": "6.0.0-beta.52",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "circular-dependency-plugin": "^5.2.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.0.0",
    "electron": "9.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.20.5",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "less-loader": "^6.2.0",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "node-loader": "^0.6.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.0"
  },
  "dependencies": {
    "antd": "^4.5.1",
    "custom-electron-titlebar": "^3.2.3",
    "electron-squirrel-startup": "^1.0.0",
    "lodash.clonedeep": "^4.5.0",
    "monaco-editor": "^0.20.0",
    "node-pty": "0.10.0-beta8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-electron-browser-view": "^3.0.0",
    "react-split": "^2.0.9",
    "svg-inline-react": "^3.2.0",
    "vscode-icons-js": "^10.1.1",
    "xterm": "4.9.0-beta.8",
    "xterm-addon-fit": "^0.4.0",
    "xterm-for-react": "^1.0.4"
  }
}
